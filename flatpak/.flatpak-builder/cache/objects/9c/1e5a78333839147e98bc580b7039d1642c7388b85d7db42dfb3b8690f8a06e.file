[Application]
name=io.github.rsvzz.csven
runtime=org.gnome.Platform/x86_64/47
sdk=org.gnome.Sdk/x86_64/47
command=csven

[Context]
shared=ipc;
sockets=x11;wayland;fallback-x11;
devices=dri;

[Extension io.github.rsvzz.csven.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=io.github.rsvzz.csven.Debug;